#include "x2_inc_spellhook"
#include "aaa_halfoutsider_inc"
#include "aaa_constants"
#include "ps_inc_advscript"

void main()
{
	if (!X2PreSpellCastCode())
    {	return;	}

    object oPC 			= OBJECT_SELF;
	object oTarget		= GetSpellTargetObject();
	location lTarget	= GetSpellTargetLocation();
	object 	oItem		= GetItemPossessedBy(oPC,"ps_essence");
	
	int nSpell 			= GetSpellId();
	int iLevel;
	int iCLev;
	
	switch (nSpell)
	{
///////////////////////////////////////////////////////////////////////////////////
		case 326 :		//Outsider Bloodline - Fiendish
			PS_OutsiderBloodlineFiendish(oPC, nSpell);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 327 :		//Outsider Bloodline - Celestial
			PS_OutsiderBloodlineCelestial(oPC, nSpell);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1475 :		//SupernaturalSight
			PS_HO_SupernaturalSight(oPC, nSpell);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1637 :		//Outsider Magic - Resurrection
			PS_HOM_Resurrection(oPC, nSpell, oTarget);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1639 :		//Planar Heritage - Half-Fiend
			PS_HO_PlanarHeritageFiend(oPC, oItem);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1640 :		//Planar Heritage - Half-Celestial
			PS_HO_PlanarHeritageCelestial(oPC, oItem);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1641 :		// Half-Outsider Immunity
			PS_HO_Immunity(oPC, nSpell);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1642 :		//Half-Outsider Resistance
			PS_HO_Resistance(oPC, nSpell);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1643 :		//Half-Outsider Damage Reduction
			PS_HO_DR(oPC, nSpell);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1644 :		//Outsider Magic
			PS_HO_OutsiderMagic(oPC, oItem);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1645 :		//Outsider Apotheosis
			PS_HO_Apotheosis(oPC, nSpell, oItem);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1646 :		//Outsider Magic - Fiendish Smiting
			PS_HOM_FiendishSmiting(oPC, nSpell);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1647 :		//Outsider Magic - Celestial Smiting
			PS_HOM_CelestialSmiting(oPC, nSpell);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1648 :		//Outsider Magic - Unholy Blight
			PS_HOM_UnholyBlight(oPC, nSpell, lTarget);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1649 :		//Outsider Magic - Holy Smite
			PS_HOM_HolySmite(oPC, nSpell, lTarget);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1650 :		//Outsider Magic - Rapture of Rupture
			PS_HOM_RaptureOfRupture(oPC, nSpell, oTarget);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1656 :		//Outsider Magic - Crown of Flame
			PS_HOM_CrownOfFlame(oPC, nSpell);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1657 :		//Outsider Magic - Fiendform
			PS_HOM_Fiendform(oPC, oTarget, nSpell);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1659 :		//Outsider Magic - Radiant Shield
			PS_HOM_RadiantShield(oPC, nSpell);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1660 :		//Outsider Magic -  Summon Fiend
			PS_HOM_SummonFiend(oPC, nSpell, lTarget);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1661 :		//Outsider Magic - Summon Celestial
			PS_HOM_SummonCelestial(oPC, nSpell, lTarget);
		break;
///////////////////////////////////////////////////////////////////////////////////
		case 1666 :		//Outsider Magic - Gutwrench
			PS_HOM_Gutwrench(oPC, nSpell, oTarget);
		break;
///////////////////////////////////////////////////////////////////////////////////
	}
}