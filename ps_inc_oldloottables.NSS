//Max 96
string GetFromRuneList(int nRandom) {
	int nRandom = Random(96) + 1;
	string sItem = "ps_rune_";
		
	if (nRandom < 10) 	{
		sItem += "0";
	}
		
	sItem += IntToString(nRandom);	
	return sItem;	
}

